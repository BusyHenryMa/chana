module chana.jpql.rats.LexerJPQL;

import JPQLBasic;

option flatten;

// ----- public entrance
public generic Token =
  DocComment
/ BlockComment
/ LineComment
/ Keyword
/ Word
/ Nl
/ Ws
/ Literal
/ CharacterRange
/ Action
/ Separator
/ Error
/ EOF
;

